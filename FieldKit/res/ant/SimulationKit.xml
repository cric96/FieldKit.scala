<?xml version="1.0" encoding="UTF-8"?>
<!-- ====================================================================== 
     build script for the SimulationKit library                                             
     ====================================================================== -->
<project name="SimulationKit" default="dist">
	<target name="dist">
		<ant dir="./" antfile="build.xml">
			<property name="project.name" value="${ant.project.name}" />
			<property name="project.version" value="01" />
		</ant>
	</target>
</project>