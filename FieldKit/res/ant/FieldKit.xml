<?xml version="1.0" encoding="UTF-8"?>
<!-- ====================================================================== 
     build script for the FieldKit base library
     ANT_OPTS="-Xmx1G" ant -f FieldKit.xml                                             
     ====================================================================== -->
<project name="FieldKit" default="dist">
	<target name="dist">
		<ant dir="./" >
			<property name="project.name" value="${ant.project.name}" />
			<property name="project.version" value="01" />
			<property name="project.src" value="../../src" />
		</ant>
	</target>
</project>

