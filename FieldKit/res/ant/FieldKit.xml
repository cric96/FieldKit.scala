<?xml version="1.0" encoding="UTF-8"?>
<!-- ====================================================================== 
     build script for the FieldKit core library                                             
     ====================================================================== -->
<project name="FieldKit" default="dist">
	<target name="dist">
		<ant dir="./" >
			<property name="project.name" value="${ant.project.name}" />
			<property name="project.version" value="01" />
			<property name="project.src" value="../../src.core" />
		</ant>
	</target>
</project>

